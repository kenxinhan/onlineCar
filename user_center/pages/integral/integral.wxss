.container {
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  background-color: #F2F2F2;
}

.banner {
  width: 100%;
  height: 450rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: cover;
  margin-bottom: 20rpx;
}

.inner-top {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #fff;
}

.num {
  font-size: 60rpx;
}

.num-desc {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  margin: 10rpx 0 20rpx;
}

.vip {
  height: 38rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
}

.tab {
  width: 460rpx;
  margin: 80rpx 140rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.tab-name {
  color: #333;
  font-size: 22rpx;
  text-align: center;
}

.list-wrapper {
  position: fixed;
  top: 470rpx;
  left: 0;
  right: 0;
  bottom: 0;
  /* height: 736rpx; */
  z-index: 99;
  box-sizing: border-box;
  /* padding: 0 30rpx 20rpx; */
  background-color: #fff;
}

.list-title {
  height: 36rpx;
  line-height: 36rpx;
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  position: relative;
  margin-left: 20rpx;
  padding: 20rpx;
}

.list-title::before {
  content: '';
  position: absolute;
  top: 20rpx;
  left: 0;
  width: 12rpx;
  height: 36rpx;
  background-color: #F11818;
  border-radius: 10rpx;
}

.scroll-box {
  width: 100%;
  position: absolute;
  top: 76rpx;
  left: 0;
  right: 0;
  bottom: 0;
  padding: 0 30rpx 30rpx;
  z-index: 99;
  box-sizing: border-box;
}

.item {
  width: 98%;
  height: 160rpx;
  box-sizing: border-box;
  padding: 20rpx;
  margin: 6rpx 0 20rpx 4rpx;
  box-shadow: 4rpx 4rpx 6rpx #eee, -4rpx -4rpx 6rpx #eee;
  border-radius: 10rpx;
}

.top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 10rpx;
}

.l {
  display: flex;
  align-items: center;
}

.l-r {
  margin-left: 20rpx;
  font-weight: bold;
}

.item-num {
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #F11818;
}

.r {
  width: 140rpx;
  height: 60rpx;
  border-radius: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #F11818;
  font-size: 24rpx;
  font-weight: bold;
  background-color: #FFC8C7;
}

.cur-r {
  background-color: #ccc;
  color: #fff;
}

.item-name {
  font-size: 28rpx;
  font-weight: bold;
}

.btm {
  font-size: 22rpx;
  color: #999;
}


.no-activity{
  margin: 80rpx auto 0;
  text-align: center;
}

.no-activity>.no-txt{
  font-size: 28rpx;
  color: #FF8008;
  margin-top: 20rpx;
}