.container {
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  background-color: #F2F2F2;
}

.banner {
  width: 100%;
  height: 450rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: cover;
  /* margin-bottom: 20rpx; */
}

.inner-top {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #fff;
}

.num {
  font-size: 60rpx;
}

.num-desc {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  margin: 10rpx 0 20rpx;
}

.vip {
  display: flex;
  align-items: center;
  font-size: 24rpx;
}

.tab {
  width: 460rpx;
  margin: 80rpx 140rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.tab-name {
  color: #333;
  font-size: 22rpx;
  text-align: center;
}

.scroll-box{
  /* height: 736rpx; */
  position: fixed;
  top: 470rpx;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #fff;
  box-sizing: border-box;
  padding-bottom: 80rpx;
}

.item{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 120rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #f2f2f2;
  padding: 20rpx 30rpx;
  color: #333;
}

.item-name{
  font-size: 28rpx;
  margin-bottom: 10rpx;
}

.item-time{
  font-size: 24rpx;
}

.r{
  font-size: 28rpx;
}

.btm{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  color: #999;
  background-color: #F2F2F2;
}

.no-data{
  margin: 0 auto;
  padding-top: 180rpx;
  text-align: center;
  color: #FF8008;
  font-size: 28rpx;
}