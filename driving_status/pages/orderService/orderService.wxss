.wleft {
  padding-right: 30rpx;
  margin-right: 30rpx;
  border-right: 1rpx solid #F2F2F2;
}

.wleft>.txt {
  font-size: 26rpx;
  color: #333;
}

.wleft>.time {
  font-size: 28rpx;
  color: #FF8008;
}

.wright {
  font-size: 28rpx;
  color: #333;
  line-height: 80rpx;
  text-align: center;
}

.line-tip {
  width: 100%;
}

/* .wait-info{
  background-color: #f2f2f2;
} */

.path-info {
  height: 112rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #333;
  border-bottom: 1rpx solid #F2F2F2;
}

.path-time {
  margin-right: 30rpx;
}

.wait-price {
  height: 166rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-shadow: 5rpx 5rpx 5rpx #F2F2F2;
}

.line-wait-price {
  height: 170rpx;
}

.wait-price>.tot {
  font-size: 32rpx;
  color: #FF8008;
  margin-bottom: 10rpx;
}

.wait-price .tot_num {
  font-size: 36rpx;
  font-weight: bold;
}


.wait-price>.desc {
  font-size: 22rpx;
  color: #999;
}

.wait-cancle {
  width: 690rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  background: linear-gradient(to right, #FF8008, #FFC837);
  margin: 16rpx auto;
  border-radius: 10rpx;
}

.walk-tips {
  display: flex;
  align-items: center;
  width: 90%;
  position: fixed;
  top: 5rpx;
  left: 50%;
  z-index: 999;
  transform: translateX(-50%);
  font-size: 32rpx;
  text-align: center;
  padding: 20rpx;
  background-color: #fff;
  border-radius: 10rpx;
  box-shadow: 2rpx 2rpx 3rpx #ccc;
}

.walk-tips>image {
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}

.walk-tips text {
  color: #ff6600;
}

.path-tips {
  height: 70rpx;
  position: fixed;
  left: 20rpx;
  top: 45%;
  z-index: 999;
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-radius: 90rpx;
  background-color: rgba(255, 255, 255, .7);
  font-size: 26rpx;
  box-shadow: 1rpx 2rpx 2rpx #999;
}

.path-tips>.path-tips-inner {
  padding-right: 30rpx;
  margin-right: 30rpx;
  /* border-right: 1rpx solid #ccc; */
}

.path-tips text {
  color: #ff9100;
  font-size: 32rpx;
}

.future-wrapper {
  background-color: #fff;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 408rpx;
  z-index: 999;
  border-bottom: 1rpx solid #f2f2f2;
}

.has-future {
  height: 50rpx;
  display: flex;
  padding: 30rpx 0;
  justify-content: center;
  align-items: center;
  color: #999;
  font-size: 32rpx;
}

/* .has-future_scan {
  bottom: 300rpx;
} */

.has-future_line {
  bottom: 304rpx;
}

.has-future>image {
  width: 40rpx;
  height: 40rpx;
  padding-right: 20rpx;
}

.line-tips {
  position: absolute;
  left: 50%;
  bottom: 364rpx;
  transform: translateX(-50%);
  z-index: 999;
  width: 690rpx;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  border-radius: 10rpx;
  text-align: center;
  color: #FF8008;
  font-size: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-sizing: border-box;
}

.line-tips1012{
  padding: 0 60rpx;
}

.EXC-line {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 80rpx;
  margin: 30rpx auto;
  box-sizing: border-box;
}

.EXC-line-str,
.EXC-line-end {
  width: 180rpx;
  color: #333;
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
}

.EXC-time {
  font-weight: bold;
  color: #333;
}

.EXC-time-city {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  margin: 30rpx 0;
}

.EXC-time-info {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  padding: 0 60rpx;
  margin-bottom: 30rpx;
}


.EXC-address {
  text-align: center;
  font-size: 28rpx;
  color: #333;
}

.EXC-address>text {
  display: inline-block;
  position: relative;
  color: #FF8008;
  margin-left: 30rpx;
}

.EXC-address>text::before {
  position: absolute;
  left: -20rpx;
  top: 55%;
  transform: translateY(-50%);
  content: '';
  width: 12rpx;
  height: 12rpx;
  background-color: #328944;
  border-radius: 50%;
}

.EXC-distance {
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  color: #333;
  font-size: 28rpx;
  padding: 10rpx 60rpx;
  box-sizing: border-box;
  z-index: 9999;
}

.EXC-distance2 {
  justify-content: center;
}

.exceed-state {
  color: #FF8008;
}

.EXC-date-num {
  width: 100%;
  position: fixed;
  top: 0;
  left: 50%;
  z-index: 999;
  transform: translateX(-50%);
  font-size: 28rpx;
  text-align: center;
  padding: 10rpx 0;
  background-color: rgba(255, 255, 255,.8);
  color: #333;
  border-radius: 10rpx;
  font-weight: bold;
}

.use-cart-info{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100vw;
  padding: 10rpx 0;
  display: flex;
  justify-content: center;
  font-size: 34rpx;
  background-color: rgba(0, 0, 0,.4);
  color: #fff;
}

.use-cart-info>.name{
  padding-right: 30rpx;
}

.movable-area{
  width: 100%;
  height: 100vh;
  position: absolute;
  left: 0;
  top: 120rpx;
  pointer-events: none;
}

.movable-view{
  width: 200rpx;
  height: 200rpx;
  z-index: 9999;
  pointer-events: auto;
}

.integral-img{
  width: 100%;
  height: 100%;
}